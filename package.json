{
  "name": "kiotviet-client-sdk",
  "version": "1.0.0",
  "description": "Node.js SDK for KiotViet Retail Public API with automatic token management and TypeScript support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"examples/**/*.{js,ts}\"",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "prepare": "npm run build",
    "docs": "typedoc --out docs/api src/index.ts"
  },
  "keywords": [
    "kiotviet",
    "retail",
    "api",
    "sdk",
    "typescript",
    "ecommerce",
    "pos"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/kiotviet-client-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/kiotviet-client-sdk/issues"
  },
  "homepage": "https://github.com/yourusername/kiotviet-client-sdk#readme",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/axios-mock-adapter": "^1.10.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.3.1",
    "typedoc": "^0.25.13",
    "typescript": "^5.8.2"
  }
}
